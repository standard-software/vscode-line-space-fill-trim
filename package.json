{
  "name": "vscode-line-space-fill-trim",
  "displayName": "Line Space Fill Trim",
  "description": "This vscode extension allows you to Fill and Trim spaces in lines.",
  "version": "1.0.0",
  "scripts": {
    "lint": "npx eslint",
    "lintfix": "npx eslint --fix",
    "pack": "npx vsce package && code --install-extension vscode-line-space-fill-trim-1.0.0.vsix",
    "": ""
  },
  "icon": "vscode-line-space-fill-trim_icon.png",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-line-space-fill-trim"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "line",
    "space",
    "fill",
    "trim"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-line-space-fill-trim.FillAllLines",
        "title": "Line Space : Fill To End : Select All Lines"
      },
      {
        "command": "vscode-line-space-fill-trim.FillTextLines",
        "title": "Line Space : Fill To End : Select Text Lines"
      },
      {
        "command": "vscode-line-space-fill-trim.TrimStart",
        "title": "Line Space : Trim Start"
      },
      {
        "command": "vscode-line-space-fill-trim.TrimEnd",
        "title": "Line Space : Trim End"
      },
      {
        "command": "vscode-line-space-fill-trim.Trim",
        "title": "Line Space : Trim"
      },
      {
        "command": "vscode-line-space-fill-trim.CutMinIndent",
        "title": "Line Space : Cut Min Indent"
      }
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "eslint": "^9.9.1",
    "globals": "^15.9.0"
  },
  "dependencies": {
    "graphemesplit": "^2.4.4"
  }
}
