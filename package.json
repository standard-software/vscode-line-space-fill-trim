{
  "name": "vscode-line-space-fill-trim",
  "displayName": "vscode-line-space-fill-trim",
  "description": "This vscode extension allows you to Fill and Trim spaces in lines.",
  "version": "0.0.1",
  "scripts": {
    "lint": "npx eslint",
    "lintfix": "npx eslint --fix",
    "pack": "npx vsce package && code --install-extension vscode-line-space-fill-trim-0.0.1.vsix",
    "": ""
  },
  "icon": "vscode-line-space-fill-trim_icon.png",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-line-space-fill-trim"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "line",
    "space",
    "fill",
    "trim"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-line-space-fill-trim.helloWorld",
        "title": "Line Space : Fill : Trim "
      }
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
